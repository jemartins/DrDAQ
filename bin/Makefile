clobber:
	@-rm drdaq 
	@-rm freeName 
	@-rm nameServer 
	@-rm picoGUI.tcl
	@-rm picoMgr
	@-rm picoReader.tcl
	@-rm requestName
	@echo drdaq/bin clobber

install:
	cp ../testing/testj004a/scripts/runtest drdaq
